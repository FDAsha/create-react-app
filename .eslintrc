{
  "plugins": ["jsx-a11y"],
  "settings": {},
  "ecmaFeatures": {
    "jsx": true
  },
  "extends": ["react-app", "react-app/jest", "plugin:jsx-a11y/strict"],
  "rules": {
    "no-console": 2,
    "quotes": [1, "single"],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "max-len": [2, 70],
    "indent": ["error", 2],
    "array-bracket-newline": [1, "always"],
    "array-element-newline": [1, "never"],
    "comma-dangle": [1, "never"],
    "jsx-quotes":[1, "prefer-single"],
    "line-comment-position": [1, { "position": "beside" }],
    "no-whitespace-before-property": 1,
    "object-curly-newline": [1, "always"]
  }
}
